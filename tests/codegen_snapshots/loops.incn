"""Test loop code generation: while, for, range"""

def test_while_loop() -> None:
    mut count: int = 0
    while count < 5:
        print(count)
        count += 1

def test_for_range_one_arg() -> None:
    for i in range(5):
        print(i)

def test_for_range_two_args() -> None:
    for i in range(1, 10):
        print(i)

def test_for_range_three_args() -> None:
    for i in range(0, 100, 10):
        print(i)

def test_for_list() -> None:
    numbers: list[int] = [1, 2, 3, 4, 5]
    for n in numbers:
        print(n)

def test_for_with_break() -> None:
    for i in range(10):
        if i == 5:
            break
        print(i)

def test_for_with_continue() -> None:
    for i in range(10):
        if i % 2 == 0:
            continue
        print(i)

def test_nested_loops() -> None:
    for i in range(3):
        for j in range(3):
            print(i * 10 + j)

def main() -> None:
    test_while_loop()
    test_for_range_one_arg()
    test_for_range_two_args()
    test_for_range_three_args()
    test_for_list()
    test_for_with_break()
    test_for_with_continue()
    test_nested_loops()
