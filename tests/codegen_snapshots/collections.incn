def test_collections() -> None:
    # Empty collections
    empty_list: List[int] = []
    empty_dict: Dict[str, int] = {}

    # List literals
    numbers: List[int] = [1, 2, 3, 4, 5]
    strings: List[str] = ["a", "b", "c"]
    # Note: Union types not yet implemented
    # mixed_ok: list[int | str] = [1, "two", 3]

    # Nested lists
    matrix: List[List[int]] = [[1, 2], [3, 4]]

    # Dict literals
    simple_dict: Dict[str, int] = {"a": 1, "b": 2}
    int_keys: Dict[int, str] = {1: "one", 2: "two"}

    # Nested dicts
    nested: Dict[str, Dict[str, int]] = {
        "first": {"x": 1, "y": 2},
        "second": {"x": 3, "y": 4}
    }

    # Tuple literals
    pair: tuple[int, str] = (1, "one")
    triple: tuple[int, int, int] = (1, 2, 3)
    multiple: Tuple = ("a", pair, "b", triple, 42)

    # Indexing
    first: int = numbers[0]
    last: int = numbers[-1]
    value: int = simple_dict["a"]

    # Slicing
    sub_list: List[int] = numbers[1:3]
    all_but_first: List[int] = numbers[1:]
    all_but_last: List[int] = numbers[:-1]


def main() -> None:
    test_collections()
