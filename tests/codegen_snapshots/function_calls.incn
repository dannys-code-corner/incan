def greet(name: str) -> str:
    """Simple function for testing calls"""
    return f"Hello, {name}!"


def add(a: int, b: int) -> int:
    """Test positional arguments"""
    return a + b


def greet_with_title(name: str, title: str = "Mr.") -> str:
    """Test default arguments"""
    return f"Hello, {title} {name}!"


def test_function_calls() -> None:
    # Simple function call
    message: str = greet("World")
    
    # Multiple arguments
    sum: int = add(5, 3)
    
    # Default arguments
    greeting1: str = greet_with_title("Smith")
    greeting2: str = greet_with_title("Jones", "Dr.")
    
    # Built-in functions
    length: int = len("hello")
    string_list: List[str] = ["a", "b", "c"]
    count: int = len(string_list)
    
    # Method calls
    upper: str = "test".upper()
    lower: str = "TEST".lower()
    stripped: str = "  spaces  ".strip()
    
    # Chained method calls
    result: str = "hello world".upper().strip()
    
    # List methods
    numbers: List[int] = [1, 2, 3]
    numbers.append(4)
    popped: int = numbers.pop()
    
    # Dict methods
    data: Dict[str, int] = {"a": 1, "b": 2}
    # Note: list() builtin not yet implemented for iterators
    # keys: list[str] = list(data.keys())
    # values: list[int] = list(data.values())


def main() -> None:
    test_function_calls()
