"""Test lowercase type names (Python-style)"""

def test_lowercase_types() -> None:
    # Lowercase collection types should work
    numbers: list[int] = [1, 2, 3]
    data: dict[str, int] = {"a": 1, "b": 2}
    unique: set[int] = {1, 2, 3}
    
    # Tuple literals 
    pair: tuple[int, str] = (42, "hello")
    
    # Lowercase option works
    maybe: option[int] = None
    
    print("All lowercase types work!")

def main() -> None:
    test_lowercase_types()
