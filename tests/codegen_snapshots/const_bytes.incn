"""Const bytes bindings (RFC 008).

This covers both:
- `bytes` annotation (lowered to a Rust-native `'static` byte slice)
- `FrozenBytes` annotation (frozen wrapper, constructed via `FrozenBytes::new(...)`)
"""

const RAW: bytes = b"\x00\x01"
const FROZEN: FrozenBytes = b"\x02\x03"

def main() -> None:
    print(RAW.len())
    print(FROZEN.len())
