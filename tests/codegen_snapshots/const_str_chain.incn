"""Chained const string concatenation.

Regression test: consts defined via concatenation must still be usable in later concatenations
without emitting invalid Rust like `B + "!"` for `&'static str`.
"""

const A: str = "hello"
const B: str = A + " world"
const C: str = B + "!"

def main() -> None:
    print(C)
    # further concatenation should be allowed
    print(C + "?")
