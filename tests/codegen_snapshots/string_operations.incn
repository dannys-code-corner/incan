"""Test string operation code generation"""

def test_string_methods() -> None:
    s: str = "Hello World"
    upper: str = s.upper()
    lower: str = s.lower()
    stripped: str = s.strip()

def test_string_contains() -> None:
    text: str = "hello world"
    has_hello: bool = text.contains("hello")
    has_goodbye: bool = text.contains("goodbye")

def test_string_split() -> None:
    text: str = "one,two,three"
    parts: list[str] = text.split(",")

def test_string_replace() -> None:
    text: str = "hello world"
    replaced: str = text.replace("world", "python")

def test_string_startswith_endswith() -> None:
    text: str = "hello.txt"
    starts: bool = text.startswith("hello")
    ends: bool = text.endswith(".txt")

def test_string_join() -> None:
    sep: str = ", "
    words: list[str] = ["one", "two", "three"]
    joined: str = sep.join(words)

def test_string_concatenation() -> None:
    mut result: str = "hello"
    result += " "
    result += "world"
    print(result)

def main() -> None:
    test_string_methods()
    test_string_contains()
    test_string_split()
    test_string_replace()
    test_string_startswith_endswith()
    test_string_join()
    test_string_concatenation()
