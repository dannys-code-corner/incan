def test_operators() -> None:
    # Basic arithmetic
    add: int = 1 + 2
    sub: int = 5 - 3
    mul: int = 4 * 6
    float_div: float = 10 / 3
    mod_result: int = 10 % 3
    
    # Comparison operators
    eq: bool = 5 == 5
    ne: bool = 3 != 4
    lt: bool = 2 < 5
    le: bool = 2 <= 2
    gt: bool = 10 > 5
    ge: bool = 10 >= 9
    
    # Logical operators
    and_result: bool = True and False
    or_result: bool = True or False
    not_result: bool = not True
    lowercase_bool: bool = true and false
    using_is: book = True is false
    
    # Unary operators
    neg_int: int = -42
    neg_float: float = -3.14
    
    # Compound expressions
    complex: int = (1 + 2) * (3 - 4)


def main() -> None:
    test_operators()
