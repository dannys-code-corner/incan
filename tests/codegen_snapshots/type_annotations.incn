"""Test type annotation code generation"""

def test_explicit_types() -> None:
    x: int = 42
    y: float = 3.14
    z: str = "hello"
    flag: bool = True

def test_collection_types() -> None:
    numbers: list[int] = [1, 2, 3]
    words: list[str] = ["a", "b", "c"]
    mapping: dict[str, int] = {"a": 1, "b": 2}

def test_option_types() -> None:
    maybe_num: Option[int] = Some(42)
    nothing: Option[int] = None
    
def test_nested_collections() -> None:
    matrix: list[list[int]] = [[1, 2], [3, 4]]
    dict_of_lists: dict[str, list[int]] = {"nums": [1, 2, 3]}

def test_function_with_return_type() -> int:
    return 42

def test_function_parameters(x: int, y: str, z: bool) -> None:
    print(x)
    print(y)
    print(z)

def main() -> None:
    test_explicit_types()
    test_collection_types()
    test_option_types()
    test_nested_collections()
    result: int = test_function_with_return_type()
    test_function_parameters(10, "test", True)
