{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "scopeName": "markdown.incan.codeblock",
  "injectionSelector": "L:text.html.markdown",
  "patterns": [
    {
      "include": "#fenced-code-block-incan"
    },
    {
      "include": "#fenced-code-block-incn"
    }
  ],
  "repository": {
    "fenced-code-block-incan": {
      "begin": "(^|\\G)(\\s*)(\\`{3,})\\s*(?i:(incan))\\s*$",
      "beginCaptures": {
        "3": { "name": "punctuation.definition.fenced.markdown" },
        "4": { "name": "fenced_code.block.language.markdown" }
      },
      "end": "(^|\\G)(\\s*)(\\3)\\s*$",
      "endCaptures": {
        "3": { "name": "punctuation.definition.fenced.markdown" }
      },
      "name": "markup.fenced_code.block.markdown",
      "contentName": "meta.embedded.block.incan source.incan",
      "patterns": [
        { "include": "source.incan" }
      ]
    },
    "fenced-code-block-incn": {
      "begin": "(^|\\G)(\\s*)(\\`{3,})\\s*(?i:(incn))\\s*$",
      "beginCaptures": {
        "3": { "name": "punctuation.definition.fenced.markdown" },
        "4": { "name": "fenced_code.block.language.markdown" }
      },
      "end": "(^|\\G)(\\s*)(\\3)\\s*$",
      "endCaptures": {
        "3": { "name": "punctuation.definition.fenced.markdown" }
      },
      "name": "markup.fenced_code.block.markdown",
      "contentName": "meta.embedded.block.incn source.incan",
      "patterns": [
        { "include": "source.incan" }
      ]
    }
  }
}
