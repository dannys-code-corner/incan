"""
Demonstrates public visibility on models and functions.

## Notes
- `pub model` implies public fields by default.
"""

pub model Point:
    x: int
    y: int
    secret: int = 0

pub def origin() -> Point:
    return Point(x=0, y=0)

def length_squared(p: Point) -> int:
    return p.x * p.x + p.y * p.y

pub def demo() -> int:
    p = origin()
    return length_squared(p)
